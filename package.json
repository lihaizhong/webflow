{
  "name": "webflow",
  "version": "0.1.0",
  "private": true,
  "main": "dist/electron/index.js",
  "scripts": {
    "serve": "vue-cli-service serve",
    "start": "cross-env NODE_ENV=development npm run build:electron && npm run serve",
    "build": "npm run build:vue && cross-env NODE_ENV=production npm run build:electron",
    "build:vue": "vue-cli-service build",
    "build:electron": "webpack --config ./build/webpack.main.js",
    "lint": "vue-cli-service lint"
  },
  "dependencies": {
    "ajv": "^6.7.0",
    "electron": "^4.0.2",
    "electron-devtools-installer": "^2.2.4",
    "lodash.clonedeep": "^4.5.0",
    "nedb": "^1.8.0",
    "reset-css": "^4.0.1",
    "shelljs": "^0.8.3",
    "uuid": "^3.3.2",
    "vue": "^2.5.22",
    "vue-router": "^3.0.2",
    "vuex": "^3.1.0"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^3.3.0",
    "@vue/cli-plugin-eslint": "^3.3.0",
    "@vue/cli-service": "^3.3.1",
    "@vue/eslint-config-prettier": "^4.0.1",
    "babel-eslint": "^10.0.1",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.26.2",
    "clean-webpack-plugin": "^1.0.1",
    "cross-env": "^5.2.0",
    "eslint": "^5.12.1",
    "eslint-plugin-vue": "^5.1.0",
    "less": "^3.9.0",
    "less-loader": "^4.1.0",
    "lint-staged": "^8.1.0",
    "vue-template-compiler": "^2.5.22",
    "webpack": "^4.29.0",
    "webpack-cli": "^3.2.1"
  },
  "gitHooks": {
    "pre-commit": "lint-staged"
  },
  "lint-staged": {
    "*.js": [
      "vue-cli-service lint",
      "git add"
    ],
    "*.vue": [
      "vue-cli-service lint",
      "git add"
    ]
  }
}
